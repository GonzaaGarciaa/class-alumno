class Alumno:
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota
    
    def is_aprobado(self):
        if self.nota > 4:
            return (f"El alumno {self.nombre} esta aprobado")
        else:
            return (f"{self.nombre} esta desaprobado")


alumno_1 = Alumno("Gonzalo", 5)
print(alumno_1.is_aprobado())
